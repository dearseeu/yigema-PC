j.hide()
}function e(p){var o="+="+f.loopWidth+"px";
l(o,-1,p)
}function h(o){l("-="+f.loopWidth+"px",1,o)
}function l(o,s,r){var t=parseInt(b.html()),q=parseInt(n.html()),p=t+s;
