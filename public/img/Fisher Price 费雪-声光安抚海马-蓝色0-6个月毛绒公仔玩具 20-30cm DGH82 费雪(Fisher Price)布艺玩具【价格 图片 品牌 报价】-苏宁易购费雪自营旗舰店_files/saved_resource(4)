e.partNumber=sn.partNumber;
for(var h=0;
h<a;
h++){var l=k[h];
l.displayStatus="1";
if(l.baseAddUpType!="3"&&(l.baseQuantifierType=="1"||l.baseQuantifierType=="3")){continue
}var b="";
if(l.baseQuantifierType=="1"){b="元"
}else{b="件";
l.bounsLimit=parseInt(l.bounsLimit)
}if(l.subOrdergifts&&l.subOrdergifts.length>0){var c=true;
var m=l.subOrdergifts.length;
for(var f=0;
f<m;
f++){if(f>4){break
}var o=l.subOrdergifts[f];
