}if(h!="-1"){o.stepsGiftList.push(l)
}if(!o.firstLeveldisplayStatus){o.firstLeveldisplayStatus=l.displayStatus
}if(!o.bonusLabel&&k=="2"&&!m){o.bonusLabel=l.bonusLabel
}}if(o.stepsGiftList.length>0&&o.activityStatus=="1"){if(!o.bonusLabel){for(var g=0;
g<c.length;
