CommonFourPage.initPlayer=function(f,e,c,a){if(scmInfo.autoPlaySwitch!="1"){c=1
}else{c=0
}if(a==31){if(typeof CommonFourPage.ppcplayerMp5!="object"&&typeof saCustomDataUtil=="object"&&typeof saCustomDataUtil.sendData=="function"){CommonFourPage.saCustomDataUtilSendData("VideoExposure","VideoId,VideoType,PrdCode,PrdLine,ShopCode","null,31,"+sn.partNumber+",fourpage,"+sn.vendorCode)
}var b=false;
if(CommonFourPage.playerCount==0){b=true
}CommonFourPage.ppcplayerMp5=new PPCLoudplayer({isAPI:true,id:f,poster:"",playSrc:e,wmode:"opaque",autoplay:c,isMute:b,playCode:"useMp4",events:{onPlay:function(){if(sn.finishPlayVideo){if(!sn.sendPlayVideo2th){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",5,null,"+sn.partNumber+",fourpage,null,"+sn.vendorCode);
sn.sendPlayVideo2th=true
}}else{if(!sn.sendPlayVideo){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",1,null,"+sn.partNumber+",fourpage,null,"+sn.vendorCode);
sn.sendPlayVideo=true
}}},onPause:function(){if(!sn.finishPlayVideo){if(CommonFourPage.ppcplayerMp5.getCurrentTime()!=CommonFourPage.ppcplayerMp5.getDuration()){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",4,"+CommonFourPage.ppcplayerMp5.getCurrentTime()+","+sn.partNumber+",fourpage,null,"+sn.vendorCode);
if(!sn.sendPlayVideo3s){if(CommonFourPage.ppcplayerMp5.getCurrentTime()>3){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",2,3,"+sn.partNumber+",fourpage,null,"+sn.vendorCode);
sn.sendPlayVideo3s=true
}}}}},onEnd:function(){$("#imgZoom .imgzoom-video .close").click();
if(!sn.finishPlayVideo){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",3,"+CommonFourPage.ppcplayerMp5.getDuration()+","+sn.partNumber+",fourpage,null,"+sn.vendorCode);
sn.finishPlayVideo=true;
if(!sn.sendPlayVideo3s){if(CommonFourPage.ppcplayerMp5.getCurrentTime()>3){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",2,3,"+sn.partNumber+",fourpage,null,"+sn.vendorCode);
sn.sendPlayVideo3s=true
}}}if(document.exitFullscreen){document.exitFullscreen()
}else{if(document.msExitFullscreen){document.msExitFullscreen()
}else{if(document.mozCancelFullScreen){document.mozCancelFullScreen()
}else{if(document.oRequestFullscreen){document.oCancelFullScreen()
}else{if(document.webkitExitFullscreen){document.webkitExitFullscreen()
}}}}}}},})
}else{if(typeof CommonFourPage.ppcplayerMp4!="object"&&typeof saCustomDataUtil=="object"&&typeof saCustomDataUtil.sendData=="function"){CommonFourPage.saCustomDataUtilSendData("VideoExposure","VideoId,VideoType,PrdCode,PrdLine,ShopCode","null,30,"+sn.partNumber+",fourpage,"+sn.vendorCode)
}CommonFourPage.ppcplayerMp4=new PPCLoudplayer({id:f,poster:"",playSrc:e,wmode:"opaque",autoplay:c,playCode:"useMp4",events:{onPlay:function(){if(sn.finishPlayVideo){if(!sn.sendPlayVideo2th){CommonFourPage.saCustomDataUtilSendData("Play","VideoId,VideoName,VideoType,PlayState,PlayTime,PrdCode,PrdLine,FirstVideoId,ShopCode","null,null,"+a+",5,null,"+sn.partNumber+",fourpage,null,"+sn.vendorCode);
