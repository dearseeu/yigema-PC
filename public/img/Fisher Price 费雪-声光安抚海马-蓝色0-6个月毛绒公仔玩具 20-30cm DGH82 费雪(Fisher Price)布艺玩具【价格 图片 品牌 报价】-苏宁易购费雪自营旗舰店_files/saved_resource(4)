p+="&freight="+j;
var i=$(".ui-city span").eq(1).html();
p+="&cityName="+i;
var w=d("idsEppLastLogin")!=null?d("idsEppLastLogin"):(d("logonUserIdLastTime")!=null?d("logonUserIdLastTime"):d("idsLoginUserIdLastTime"));
p+="&userNmae="+w;
p+="&compete="+v;
p+="&competePrice="+a;
p+="&competeFreight="+l;
p+="&competeCityname="+c;
p+="&competeTS="+b;
p=encodeURI(p);
