p+="&rushPrice="+u;
var o=sn.priceType=="4"&&PriceShow.serviceType=="3"?sn.promotionPrice:"";
p+="&groupPrice="+o;
var k=sn.priceType=="1"?sn.promotionPrice:"";
p+="&promotionPrice="+k;
var e=sn.priceType=="0"?sn.promotionPrice:"";
p+="&netPrice="+e;
