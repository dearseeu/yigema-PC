}}else{if(sn.swlShopFlag){$("#shopName").html("由&nbsp;<span>苏宁</span>&nbsp;销售和发货，并提供售后服务")
}else{if(sn.csSwlShopFlag){$("#shopName").html("由&nbsp;<span>苏宁自营</span>&nbsp;销售和发货，并提供售后服务")
}else{if(sn.hasStorage=="Y"&&sn.sendCityName&&sn.sendCityName!=""){if(sn.csendBySn){$("#shopName").html("由&nbsp;苏宁&nbsp;从 &nbsp;"+sn.sendCityName+"&nbsp;发货，"+sn.curShopName+"提供售后服务")
}else{$("#shopName").html("由&nbsp;"+sn.curShopName+"&nbsp;从 &nbsp;"+sn.sendCityName+"&nbsp;销售和发货，并提供售后服务")
}}else{if(sn.csendBySn){$("#shopName").html("由&nbsp;苏宁&nbsp;发货，"+sn.curShopName+"提供售后服务")
}else{$("#shopName").html("由&nbsp;"+sn.curShopName+"&nbsp;销售和发货，并提供售后服务")
}}}}}}else{var b=$("#shop_code").val();
var e=$("#shop_status").val();
