var c=$("#brand_dacu_url").val();
var a;
if(c&&c!=""){a=c
}else{a=sn.shopPath+sn.shopMainPh+"/"+b.substring(2,10)+"/index.html"
}var f="，并提供售后服务";
if(sn.brandId=="00028"){$("#shopName").hide()
}if(sn.factorySendFlag=="1"){f="，苏宁提供售后服务";
