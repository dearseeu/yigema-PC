}a+='<span class="tsg">'+sn.pavilion+"</span>"
}else{if(sn.zyHwgFlag||sn.czyHwgFlag){if(sn.snZhenx&&sn.snZhenxLogo){a='<img class="snzx-common" src="'+sn.snZhenxLogo+'">'
}else{a='<span class="hwg"><i>苏宁国际</i><i class="hwg-zy-in">直营</i></span>'
}}else{if(sn.shopType=="3"){if(sn.snZhenx&&sn.snZhenxLogo){a='<img class="snzx-common" src="'+sn.snZhenxLogo+'">'
}else{a='<span class="hwg">苏宁国际</span>'
}}else{if(sn.supermarketCheck&&(sn.shopType=="0"||sn.shopType=="4"||sn.shopType=="5")){if(sn.snZhenx&&sn.snZhenxLogo){a='<img class="snzx-common" src="'+sn.snZhenxLogo+'">'
}else{a='<span class="cs"><i>苏宁超市</i><i class="cs-zy">自营</i></span>'
}}else{if(sn.vendorCode==""||sn.vendorCode=="0000000000"||sn.businessType=="0"||sn.csSwlShopFlag||sn.tmShopFlag){if(sn.snZhenx&&sn.snZhenxLogo){a='<img class="snzx-common" src="'+sn.snZhenxLogo+'">'
