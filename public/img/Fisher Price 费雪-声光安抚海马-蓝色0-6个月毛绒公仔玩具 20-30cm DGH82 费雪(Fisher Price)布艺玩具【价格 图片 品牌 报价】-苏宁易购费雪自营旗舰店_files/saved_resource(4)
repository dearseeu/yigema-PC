}l.subOrdergifts[f].imgUrl=sn.newImageDomianDir+"/uimg/b2c/newcatentries/"+sn.vendorCode+"-"+o.giftCode+"_1_60x60.jpg";
g+=(g!=""?"_":"")+sn.vendorCode+"-"+o.giftCode
}if(l.subOrdergifts.length>1){var q=l.subOrdergifts.length;
if(l.giftsModelNumber<q){q=l.giftsModelNumber
}l.manzenPop="满"+l.bounsLimit+b+"领取任意"+q+"种赠品（赠完即止）"
}else{l.manzenPop="满"+l.bounsLimit+b+"领取以下赠品（赠完即止）"
}if(!e.manzenTitle&&!e.manzenPop){e.manzenTitle="满"+l.bounsLimit+b+"可到购物车领取赠品（赠完即止）";
e.manzenPop=l.manzenPop
}if(c){l.displayStatus="0"
}if(!e.firstLeveldisplayStatus){e.firstLeveldisplayStatus=l.displayStatus
}e.ordersGiftList.push(l)
}}if(e.ordersGiftList.length>0){$.ajax({url:sn.itemDomain+"/pds-web/ajax/batchMainPicutures/"+g+".html",type:"get",dataType:"json",success:function(t){var A={};
