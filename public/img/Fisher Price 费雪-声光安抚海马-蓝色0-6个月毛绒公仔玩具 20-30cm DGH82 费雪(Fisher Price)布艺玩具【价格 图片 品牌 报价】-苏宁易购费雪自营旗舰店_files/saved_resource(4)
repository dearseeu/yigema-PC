iFourth.singleloop=function(c){var f={wrap:"",loopBox:"",triggerLeft:".prev",triggerRight:".next",curCount:".cur-count",totalCount:".total-count",loopWidth:180,prePageWide:5,prePageNarrow:4,isLoop:false};
