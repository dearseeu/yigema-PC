}else{c=sn.shopType
}var b=sn.solsUrl+"/sols/http/SOLP10521_"+sn.lesCityId+"_50_"+a+"_"+e+"_"+sn.netPrice+"_"+sn.catenIds+"_"+sn.brandId+"_"+c+"_queryCStoreWarranty/"+f+".htm";
ajaxICPSPromInfo(b,f)
}function getSOLPInsureInfo(g,h){if("0"==scmInfo.cShopBaoxianSwitch){return
}var a=sn.vendorCode==""?"0000000000":sn.vendorCode;
