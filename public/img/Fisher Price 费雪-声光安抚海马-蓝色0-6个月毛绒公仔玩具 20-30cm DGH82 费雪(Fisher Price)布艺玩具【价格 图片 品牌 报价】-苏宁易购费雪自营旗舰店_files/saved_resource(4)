FourPage.analyzeCookie=function(e){var b=e.split("|");
var a=null;
if(b.length>0){var c=b[0].split("_");
a={};
a.provinceMDMId=c[0];
a.provinceCommerceId=c[0];
a.cityLESId=c[1];
a.cityMDMId=c[2];
a.cityCommerceId=c[3];
a.districtMDMId=c[4];
a.districtLESId=c[4];
a.districtCommerceId=c[5];
a.flag=c[6];
a.count=c[7]
}return a
};
