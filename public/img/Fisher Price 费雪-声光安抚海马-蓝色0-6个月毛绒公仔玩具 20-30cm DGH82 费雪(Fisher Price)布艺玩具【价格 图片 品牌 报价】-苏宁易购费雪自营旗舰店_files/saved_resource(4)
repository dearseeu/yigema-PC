yanBaoQuan.couponList.push(a)
}}}};
yanBaoQuan.parseConponInfo=function(c){var a={};
a.activityId=c.activityId;
a.ticketId=c.activityId;
a.ticketCode=c.activitySecretKey;
var f=c.bonusList[0];
var b=f.couponValue;
if(b){b+="";
if(b.indexOf(".")!=-1){var e=b.split(".");
a.priceInteger=e[0];
a.priceDecimal="."+e[1]
}else{a.priceInteger=b;
a.priceDecimal=".00"
}}else{a.priceInteger="0";
a.priceDecimal=".00"
}a.ticketDesc=f.couponPromotionLabel;
a.timeRegion=getFormatYYMMDD(f.couponStartTime)+"至"+getFormatYYMMDD(f.couponEndTime);
a.receiveQuan="yanBaoQuan.receiveQuan('"+a.ticketCode+"','"+a.ticketId+"','"+a.ticketId+"')";
return a
};
